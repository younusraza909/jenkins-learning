pipeline{
    agent any

    stages{
        stage("build jar"){
            steps{
                script{
                    echo "Building Jar File"
                }  
            }
        }

        stage("build image"){
            steps{
                script{
                    echo "Building Docker Image"
                }  
            }
        }

        stage("deploy build"){
            steps{
                script{
                    echo "Deploying  Build"
                }  
            }
        }
    }
}